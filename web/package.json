{
    "name": "intel8080-emu",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "test": "vue-cli-service test:unit",
        "cover": "vue-cli-service test:unit  --coverage"
    },
    "dependencies": {
        "monaco-editor": "0.18.1",
        "vue": "2.6.10",
        "vue-property-decorator": "8.3.0",
        "vue-router": "3.1.3"
    },
    "devDependencies": {
        "@types/jest": "24.0.21",
        "@vue/cli-plugin-typescript": "4.0.5",
        "@vue/cli-plugin-unit-jest": "^4.0.5",
        "@vue/cli-service": "4.0.5",
        "@vue/test-utils": "1.0.0-beta.29",
        "babel-core": "6.26.3",
        "compression-webpack-plugin": "^3.0.0",
        "jest": "24.9.0",
        "ts-jest": "24.1.0",
        "typescript": "3.6.4",
        "vue-class-component": "7.1.0",
        "vue-template-compiler": "2.6.10"
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "transform": {
            "^.+\\.ts$": "ts-jest"
        },
        "moduleNameMapper": {
            "^@/(.*)$": "<rootDir>/src/$1"
        },
        "snapshotSerializers": [
            "jest-serializer-vue"
        ],
        "testMatch": [
            "**/*.test.ts"
        ]
    }
}
