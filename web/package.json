{
    "name": "intel8080-emu",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "test": "vue-cli-service test:unit",
        "cover": "vue-cli-service test:unit  --coverage"
    },
    "dependencies": {
        "monaco-editor": "0.21.1",
        "vue": "2.6.12",
        "vue-property-decorator": "9.0.0",
        "vue-router": "3.4.4"
    },
    "devDependencies": {
        "@babel/core": "^7.11.6",
        "@types/jest": "26.0.14",
        "@vue/cli-plugin-typescript": "4.5.6",
        "@vue/cli-plugin-unit-jest": "^4.5.6",
        "@vue/cli-service": "4.5.6",
        "@vue/test-utils": "1.1.0",
        "jest": "26.4.2",
        "ts-jest": "26.4.0",
        "typescript": "4.0.3",
        "vue-class-component": "7.2.6",
        "vue-template-compiler": "2.6.12"
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "transform": {
            "^.+\\.ts$": "ts-jest"
        },
        "moduleNameMapper": {
            "^@/(.*)$": "<rootDir>/src/$1"
        },
        "snapshotSerializers": [
            "jest-serializer-vue"
        ],
        "testMatch": [
            "**/*.test.ts"
        ]
    }
}
