{
    "name": "intel8080-emu",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "test": "vue-cli-service test:unit",
        "cover": "vue-cli-service test:unit  --coverage"
    },
    "dependencies": {
        "monaco-editor": "0.19.3",
        "vue": "2.6.11",
        "vue-property-decorator": "8.3.0",
        "vue-router": "3.1.5"
    },
    "devDependencies": {
        "@babel/core": "^7.8.3",
        "@types/jest": "24.9.1",
        "@vue/cli-plugin-typescript": "4.1.2",
        "@vue/cli-plugin-unit-jest": "^4.1.2",
        "@vue/cli-service": "4.1.2",
        "@vue/test-utils": "1.0.0-beta.31",
        "jest": "25.1.0",
        "ts-jest": "25.0.0",
        "typescript": "3.7.5",
        "vue-class-component": "7.2.2",
        "vue-template-compiler": "2.6.11"
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "transform": {
            "^.+\\.ts$": "ts-jest"
        },
        "moduleNameMapper": {
            "^@/(.*)$": "<rootDir>/src/$1"
        },
        "snapshotSerializers": [
            "jest-serializer-vue"
        ],
        "testMatch": [
            "**/*.test.ts"
        ]
    }
}
